! rxvt-unicode settings
!! rxvt-unicode behaviour
URxvt*cutchars: `"()'*<>[]{|}
urxvt*perl-lib: /usr/lib/urxvt/perl/

URxvt.urgentOnBell: true
URxvt.buffered: true
URxvt*termName: rxvt
URxvt*scrollBar: false
URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true
URxvt*secondaryScroll: true
URxvt*jumpScroll: true
URxvt*skipScroll: true
URxvt*keysym.M-u: perl:mark-yank-urls:activate_mark_mode
URxvt*perl-ext: default,matcher,mark-yank-urls
URxvt*saveLines: 500
URxvt*visualBell: true
URxvt*highlightSelection: true
URxvt*borderWidth: 0
URxvt*urlLauncher: fxs
URxvt*pointerBlank: true
URxvt*internalBorder: 3
URxvt.perl-ext-common: default,matcher
URxvt.urlLauncher: conkeror
URxvt.matcher.button: 1
URxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@#-]*[\\w/-]

!! rxvt-unicode appearance
URxvt*background: black
URxvt*foreground: white
URxvt*cursorColor: white
URxvt*cursorBlink: false

!!rxvt-unicode fonts
URxvt.font: xft:Terminus:pixelsize=13:antialias=true
URxvt.boldFont: xft:Terminus:bold:pixelsize=13:antialias=true

!URxvt.font: -*-terminus-medium-*-*-*-*-120-*-*-*-*-*-*
!URxvt.boldFont: -*-terminus-bold-*-*-*-*-120-*-*-*-*-*-*

! xterm settings
xterm*background: #00000
xterm*foreground: #FFFFF
xterm.font: -*-terminus-medium-*-*-*-*-120-*-*-*-*-*-*

